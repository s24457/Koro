#include <iostream>
using namespace std;

auto main (int argc, char** argv) -> int {
    auto const a = stoi(argv[1]);
    auto const b = stoi(argv[2]);

    for (int i = 0; i < a; i++){
        for (int j = 0; j < b; ++j) {
            cout << "*";
        }
        cout << endl;
    }
    return 0;
}
