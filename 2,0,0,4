#include <iostream>
using namespace std;

auto main (int argc, char** argv) -> int {
   auto a = stoi(argv[1]);
   int f = 0;
    for (int i = 2; i < a/2 ; ++i) {
        if (a % i == 0)
            f++;
    }
    if (f == 0)
        cout << Tak"\n";
    else
        cout << "Nie\n";
    return 0;
}
