#include <iostream>
using namespace std;

auto main (int argc, char** argv) -> int {
    auto const a = stoi(argv[1]); //lim
    auto const b = stoi(argv[2]); // dziel
    auto f = 0;
    for (int i = 1; i <a ; ++i) {
        if (i % b == 0)
            f+=i;
    }
    cout << f << endl;
    return 0;
}
