#include <iostream>
using namespace std;

auto main (int argc, char** argv) -> int {
    auto a = stoi(argv[1]);
    auto b = stoi(argv[2]);
    if (a > b)
        cout << a << " > " << b << endl;
    else if (a < b)
        cout << a << " < " << b << endl;
    else
        cout << a << " = " << b << endl;
    return 0;
}
